type Apartment {
  id: ID! @unique
  name: String!
  image: String
  places: [Place]!
  # owner: User!
  # tenants: [User]!
  createdAt: DateTime!
  updatedAt: DateTime!
}

type User {
  id: ID! @unique
  email: String! @unique
  password: String!
  name: String!
}

type Place {
  id: ID! @unique
  name: String!
  difficulty: Int!
  createdAt: DateTime!
  updatedAt: DateTime!
  apartment: Apartment!
}

type IWasntThere {
  id: ID! @unique
  date: DateTime!
  user: User!
  apartment: Apartment!
}
